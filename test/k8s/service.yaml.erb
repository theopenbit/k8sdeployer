apiVersion: v1
kind: Service
metadata:
  labels:
    application: demo-nginx
    variant: v<%= namespaceconfig['variant'] %>
  name: demo-nginx
  namespace: <%= namespace %> 
spec:
  selector:
    application: demo-nginx
  type: NodePort
  ports:
  - port: 80
    name: http
    protocol: TCP
    targetPort: 80
  - port: 443
    name: https
    protocol: TCP
    targetPort: 443    
